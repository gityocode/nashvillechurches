<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCmBnKTeHu-tkSurV2BBc22Rpt9XeEZOv8&callback=initMap"></script>

<script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js"></script>

<script>
function initMap() {
    const map = new google.maps.Map(document.getElementById('map'), {
      zoom: 11,
      center: {lat: 36.1608183, lng: -86.7830144}, 
      mapId: '18bb2aceb768448d' 
    });

    var markers = [];

    // Load your GeoJSON data
    fetch('https://raw.githubusercontent.com/gityocode/nashvillechurches/main/church_json%20(4).geojson')
      .then(function(response){
        return response.json();
      })
      .then(function(data){
        // For each feature in the GeoJSON data...
        data.features.forEach(function(feature){
          // Create a marker
          var marker = new google.maps.Marker({
            position: {
              lat: feature.geometry.coordinates[1],
              lng: feature.geometry.coordinates[0]
            },
            map: map
          });

          // And add it to the markers array
          markers.push(marker);
        });

        // Add marker clustering
        new MarkerClusterer(map, markers, {imagePath: 'https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/m'});
      });
  }

</script>
